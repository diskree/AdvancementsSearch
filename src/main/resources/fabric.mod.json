{
	"schemaVersion": 1,
	"id": "${modId}",
	"version": "${modVersion}",
	"name": "${modName}",
	"description": "A client-side mod that adds a search feature to the advancements window.",
	"authors": [
		"${author}"
	],
	"contact": {
		"sources": "${repoUrl}"
	},
	"license": "MIT",
	"icon": "assets/advancementssearch/icon.png",
	"environment": "client",
	"entrypoints": {
		"client": [
			"com.diskree.advancementssearch.AdvancementsSearch"
		]
	},
	"mixins": [
		"advancementssearch.mixins.json"
	],
	"depends": {
		"java": ">=${minJavaVersion}",
		"minecraft": ">=${minMinecraftVersion}",
		"fabricloader": ">=${minLoaderVersion}",
		"fabric-api": "*"
	},
	"accessWidener": "advancementssearch.accesswidener"
}
